<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="@color/ButtonUnavailableStroke"/>
            <solid android:color="@color/ButtonUnavailable"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <solid android:color="@color/ButtonUnavailablePressed"/>
            <stroke
                android:width="1dp"
                android:color="@color/ButtonUnavailableStrokePressed"/>
        </shape>
    </item>
</selector>