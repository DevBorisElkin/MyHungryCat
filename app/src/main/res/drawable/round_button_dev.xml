<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="@color/DarkPink4"/>
            <solid android:color="@color/DarkPink3"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <solid android:color="@color/DarkPink2"/>
            <stroke
                android:width="1dp"
                android:color="@color/DarkPink1"/>
        </shape>
    </item>
</selector>