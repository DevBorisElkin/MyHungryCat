<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Levels.Level_six"
    android:background="@drawable/b8"
    android:id="@+id/layout">


    <ImageView
        android:id="@+id/fox"
        android:layout_width="88dp"
        android:layout_height="94dp"
        android:layout_marginTop="68dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/rabbit"
        app:srcCompat="@drawable/fox" />

    <ImageView
        android:id="@+id/mole"
        android:layout_width="102dp"
        android:layout_height="87dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toTopOf="@+id/bush"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/mole_ultimate_reverse" />

    <ImageView
        android:id="@+id/stomp"
        android:layout_width="63dp"
        android:layout_height="51dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="60dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fox"
        app:srcCompat="@drawable/stump" />

    <ImageView
        android:id="@+id/owl"
        android:layout_width="58dp"
        android:layout_height="72dp"
        android:layout_marginBottom="44dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/stomp"
        app:layout_constraintStart_toStartOf="@+id/stomp"
        app:layout_constraintTop_toBottomOf="@+id/fox"
        app:layout_constraintVertical_bias="0.615"
        app:srcCompat="@drawable/owl" />

    <ImageView
        android:id="@+id/cat"
        android:layout_width="56dp"
        android:layout_height="62dp"
        android:layout_marginBottom="132dp"
        android:layout_marginEnd="60dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/mole"
        app:srcCompat="@drawable/hungry_cat" />



    <ImageView
        android:id="@+id/itemSpawn"
        android:layout_width="30dp"
        android:layout_height="27dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/sign"
        android:layout_width="187dp"
        android:layout_height="176dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toEndOf="@+id/tree2"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/sign1" />

    <ImageView
        android:id="@+id/table"
        android:layout_width="151dp"
        android:layout_height="96dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="124dp"
        app:layout_constraintStart_toEndOf="@+id/tree2"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bear_table" />

    <ImageView
        android:id="@+id/chest2"
        android:layout_width="94dp"
        android:layout_height="48dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintEnd_toEndOf="@+id/tree1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/chest1"
        app:layout_constraintTop_toTopOf="@+id/chest1"
        app:srcCompat="@drawable/forest_box" />

    <ImageView
        android:id="@+id/chest1"
        android:layout_width="94dp"
        android:layout_height="48dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="36dp"
        app:layout_constraintStart_toEndOf="@+id/tree2"
        app:layout_constraintTop_toBottomOf="@+id/cashier"
        app:srcCompat="@drawable/forest_box" />

    <ImageView
        android:id="@+id/cashier"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="72dp"
        android:layout_marginStart="32dp"
        app:layout_constraintBottom_toBottomOf="@+id/table"
        app:layout_constraintStart_toStartOf="@+id/table"
        app:srcCompat="@drawable/cashier" />


    <ImageView
        android:id="@+id/coin5"
        android:layout_width="38dp"
        android:layout_height="20dp"
        android:layout_marginBottom="92dp"
        app:layout_constraintBottom_toBottomOf="@+id/tree1"
        app:layout_constraintStart_toStartOf="@+id/tree1"
        app:srcCompat="@drawable/coin_five"
        />

    <ImageView
        android:id="@+id/somethingBehindTree2"
        android:layout_width="28dp"
        android:layout_height="30dp"
        android:layout_marginBottom="84dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/tree2"
        app:layout_constraintEnd_toEndOf="@+id/tree2"
        app:srcCompat="@drawable/apple" />

    <ImageView
        android:id="@+id/bush"
        android:layout_width="143dp"
        android:layout_height="122dp"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/bush" />

    <ImageView
        android:id="@+id/tree1"
        android:layout_width="101dp"
        android:layout_height="286dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/tree" />
    <ImageView
        android:id="@+id/restart"
        android:layout_width="50dp"
        android:layout_height="47dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/button_circle_3"
        android:elevation="5dp"
        android:padding="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/restart" />

    <ImageView
        android:id="@+id/tree2"
        android:layout_width="77dp"
        android:layout_height="222dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/tree2" />
    <LinearLayout
        android:id="@+id/coin_counter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/btn_yellow_style"
        android:orientation="horizontal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:paddingStart="4dp"
            android:text="@string/coin_count"
            android:textColor="@color/white"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/moneyCount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:text="0"
            android:textColor="@color/white"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="30dp"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:paddingEnd="4dp"
            app:srcCompat="@drawable/coin" />
    </LinearLayout>
    <ImageView
        android:id="@+id/rabbit"
        android:layout_width="53dp"
        android:layout_height="42dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="60dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tree2"
        app:srcCompat="@drawable/rabbit" />

    <ImageView
        android:id="@+id/squirrel"
        android:layout_width="38dp"
        android:layout_height="41dp"
        android:layout_marginBottom="180dp"
        app:layout_constraintBottom_toBottomOf="@+id/tree1"
        app:layout_constraintStart_toStartOf="@+id/tree1"
        app:srcCompat="@drawable/squirrel" />

    <ImageView
        android:id="@+id/pinecone1"
        android:layout_width="27dp"
        android:layout_height="32dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toStartOf="@+id/tree1"
        app:layout_constraintTop_toBottomOf="@+id/tree1"
        app:srcCompat="@drawable/pinecone" />

    <ImageView
        android:id="@+id/plant"
        android:layout_width="27dp"
        android:layout_height="55dp"
        android:layout_marginBottom="75dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/table"
        app:layout_constraintEnd_toEndOf="@+id/table"
        app:srcCompat="@drawable/plant_good" />

    <ImageView
        android:id="@+id/pinecone2"
        android:layout_width="36dp"
        android:layout_height="27dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="184dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/pinecone1"
        app:srcCompat="@drawable/pinecone_pushed" />

    <ImageView
        android:id="@+id/pinecone3"
        android:layout_width="27dp"
        android:layout_height="32dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toTopOf="@+id/pinecone2"
        app:layout_constraintEnd_toStartOf="@+id/pinecone2"
        app:srcCompat="@drawable/pinecone" />

    <ImageView
        android:id="@+id/key"
        android:layout_width="25dp"
        android:layout_height="36dp"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toTopOf="@+id/pinecone2"
        app:layout_constraintStart_toStartOf="@+id/pinecone2"
        app:srcCompat="@drawable/key" />

    <ImageView
        android:id="@+id/carrot"
        android:layout_width="38dp"
        android:layout_height="39dp"
        android:layout_marginEnd="28dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toStartOf="@+id/pinecone2"
        app:layout_constraintTop_toBottomOf="@+id/pinecone3"
        app:srcCompat="@drawable/carrot" />

    <ImageView
        android:id="@+id/squirrel_falling"
        android:layout_width="38dp"
        android:layout_height="41dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toEndOf="@+id/pinecone2"
        app:layout_constraintTop_toBottomOf="@+id/pinecone3"
        app:srcCompat="@drawable/squirrel_back2" />


</android.support.constraint.ConstraintLayout>